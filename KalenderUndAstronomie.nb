Cell[TextData[{
 "The ",
 StyleBox["Separation",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " command is useful for determining Conjunctions, Eclipses and Transits etc."
}], "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"29dbdd80-bc17-47d5-9cc0-abf14496ac10"],

Cell[CellGroupData[{

Cell[BoxData[
