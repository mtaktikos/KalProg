Cell["\<\
Wie viel Prozent alle Neujahrstage fallen in die 1., wie viel
in die 52. und wie viel in die 53. Kalenderwoche?\
\>", "Text",ExpressionUUID->"81693d53-d221-40d1-9ef9-a7144509efc5"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Module", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"akt", ",", 
     RowBox[{"fuenfzwo", "=", "0"}], ",", 
     RowBox[{"fuenfdrei", "=", "0"}], ",", 
     RowBox[{"eins", "=", "0"}], ",", " ", "alle"}], "}"}], ",", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Do", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"akt", "=", 
        RowBox[{"CWeek", "[", 
         RowBox[{"ConvertDateTo", "[", 
          RowBox[{
           RowBox[{"Gregorian", "[", 
            RowBox[{"j", ",", "1", ",", "1"}], "]"}], ",", "ISO"}], "]"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Which", "[", 
        RowBox[{
         RowBox[{"akt", " ", "\[Equal]", " ", "1"}], ",", 
         RowBox[{"eins", " ", "+=", " ", "1"}], ",", "\[IndentingNewLine]", 
         "            ", 
         RowBox[{"akt", " ", "\[Equal]", " ", "52"}], ",", " ", 
         RowBox[{"fuenfzwo", " ", "+=", " ", "1"}], ",", 
         "\[IndentingNewLine]", "            ", 
         RowBox[{"akt", "\[Equal]", " ", "53"}], ",", " ", 
         RowBox[{"fuenfdrei", " ", "+=", " ", "1"}]}], "]"}]}], 
      "\[IndentingNewLine]", ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "2001", ",", "2400"}], "}"}]}], "]"}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"alle", "=", 
     RowBox[{"eins", "+", "fuenfzwo", "+", "fuenfdrei"}]}], ";", 
    RowBox[{"Print", "[", 
     RowBox[{"{", 
      RowBox[{"alle", ",", "eins", ",", "fuenfzwo", ",", "fuenfdrei"}], "}"}],
      "]"}], ";", "\[IndentingNewLine]", 
    RowBox[{"N", "[", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"eins", "/", "alle"}], ",", 
       RowBox[{"fuenfzwo", "/", "alle"}], ",", 
       RowBox[{"fuenfdrei", "/", "alle"}]}], "}"}], "]"}]}]}], "]"}]], "Input",
