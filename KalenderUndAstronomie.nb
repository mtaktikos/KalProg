      ",", "start", ",", "fi", ",", " ", 
      RowBox[{"erg", "=", 
       RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", " ", 
      RowBox[{
       RowBox[{"j", "<", " ", "394"}], ",", " ", 
       RowBox[{
        RowBox[{"start", "=", 
         RowBox[{"ToFixed", "[", 
          RowBox[{"Gregorian", "[", 
           RowBox[{
            RowBox[{"j", "+", "1"}], ",", "6", ",", "21"}], "]"}], "]"}]}], 
        ";", 
        RowBox[{"fi", "=", 
         RowBox[{"FullMoonAfter", "[", "start", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"erg", "=", 
         RowBox[{"Julian", "[", 
          RowBox[{"Floor", "[", "fi", "]"}], "]"}]}]}], ",", 
       RowBox[{
