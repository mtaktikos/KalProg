Cell[BoxData[
 RowBox[{"ankunftashura", " ", "=", 
  RowBox[{"Union", "[", 
   RowBox[{"Map", "[", 
    RowBox[{"ToFixed", ",", " ", 
     RowBox[{"Select", "[", 
      RowBox[{
       RowBox[{"Map", "[", 
        RowBox[{"Qussay", ",", "ankunftyomkippur"}], "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "2", "]"}], "]"}], "==", "1"}], " ", "&&", " ", 
