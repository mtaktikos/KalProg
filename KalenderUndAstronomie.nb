     RowBox[{"erg", "=", 
      RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"Do", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"d", "=", 
        RowBox[{"ToFixed", "[", 
         RowBox[{"Gregorian", "[", 
          RowBox[{"j", ",", "9", ",", "17"}], "]"}], "]"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"test", "=", 
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{"d", "+", "t"}], ",", 
          RowBox[{"{", 
           RowBox[{"t", ",", "0", ",", "8", ",", "1"}], "}"}]}], "]"}]}], ";",
        "\[IndentingNewLine]", 
       RowBox[{"sl", "=", 
        RowBox[{"First", "[", 
         RowBox[{"MinimalBy", "[", 
          RowBox[{"test", ",", 
           RowBox[{
            RowBox[{"SolarLongitudeC", "[", "#", "]"}], "&"}]}], "]"}], 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ang", "=", 
        RowBox[{"LunarPhase", "[", "sl", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Abs", "[", 
           RowBox[{"180", "-", "ang"}], "]"}], "<", "1"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"g", "=", 
           RowBox[{"Gregorian", "[", "sl", "]"}]}], ";", 
          RowBox[{"Print", "[", "g", "]"}], ";", "\[IndentingNewLine]", 
          RowBox[{"erg", "=", 
           RowBox[{"Append", "[", 
            RowBox[{"erg", ",", "g"}], "]"}]}]}]}], "]"}]}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", 
        RowBox[{"-", "6000"}], ",", 
        RowBox[{"-", "500"}]}], "}"}]}], "]"}], ";", "erg"}]}], 
  "]"}]], "Input",
