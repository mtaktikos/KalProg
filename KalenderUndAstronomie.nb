Cell[TextData[{
 "Because the ",
 StyleBox["Moon",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " is so close to ",
 StyleBox["Earth",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", you may sometimes want to set your viewpoint to the surface of ",
 StyleBox["Earth",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " (i.e ",
 StyleBox["TopoCentric",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ") rather than the default centre of the ",
 StyleBox["Earth",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 "."
}], "Text",
