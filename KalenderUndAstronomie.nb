Cell[CellGroupData[{

Cell["The EquatorCoordinates command", "Subsection",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"8bfec124-6f9f-4c42-bb57-74e52b6e9530"],

Cell["\<\
Equator Coordinates are useful in conjunction with Star charts.\
\>", "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"cd6ad874-311c-483b-afdf-22eaa35a8de3"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Information", "[", 
  RowBox[{"\"\<EquatorCoordinates\>\"", ",", 
   RowBox[{"LongForm", "\[Rule]", "False"}]}], "]"}]], "Input",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"d17b4e8a-5ce7-4640-9072-27668c2aed05"],

Cell[BoxData[
 StyleBox["\<\"EquatorCoordinates[object, date] returns the Ascension, \
Declination and Distance of the object (a planet or a star) on the given \
date. An example planet would be Mars, and an example star would be \
Alpha.Centaurus. EquatorCoordinates[horizoncoords, date] will convert the \
horizoncoords for the current location into equatorcoords. An example \
horizoncoords would be {Azimuth->270*Degree, Altitude->30*Degree}. If date is \
omitted the current Date[] is used.\"\>", "MSG"]], "Print", "PrintUsage",
 CellChangeTimes->{3.601553631021992*^9},
 CellTags->
  "Info3601557230-3591912",ExpressionUUID->"8dbeeedf-4331-4deb-a340-\
6e80bd924887"]
}, Open  ]],

Cell[TextData[{
 "The ",
 StyleBox["EquatorCoordinates",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " command can be applied to objects like ",
 StyleBox["Mars",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", ",
 StyleBox["Moon",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", ",
 StyleBox["Io",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " (solar system objects), ",
 StyleBox["Sirius",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", ",
 StyleBox["Alpha.Centaurus",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " (stars), ",
 StyleBox["Leo",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", ",
 StyleBox["UrsaMajor",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " (constellations), ",
 StyleBox["SouthCelestialPole",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 ", ",
 StyleBox["Zenith",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " (special objects) etc."
}], "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"8f40b45a-d2d3-45f1-bc73-20134143f504"],

Cell["Here are some basic examples:", "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"11f4483e-072d-4c0e-9725-528c6cc04732"],

