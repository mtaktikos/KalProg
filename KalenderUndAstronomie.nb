Cell[TextData[{
 "A command related to ",
 StyleBox["HorizonCoordinates",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " is ",
 StyleBox["Refract",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 "."
}], "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"f04c9f4c-337f-46b6-b64e-6f613c5f9e40"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Information", "[", 
  RowBox[{"\"\<Refract\>\"", ",", 
   RowBox[{"LongForm", "\[Rule]", "False"}]}], "]"}]], "Input",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"e0dbc3d7-a869-4f48-ad9d-fcc0fe58097e"],

Cell[BoxData[
 StyleBox["\<\"Refract[horizoncoords] converts true horizoncoords (i.e the \
horizon coordinates returned by the HorizonCoordinates command) into apparent \
horizon coordinates by adding an atmospheric refraction correction. The \
correction can amount to about 0.5 Degree for objects close to the horizon, \
but is otherwise quite small.\"\>", "MSG"]], "Print", "PrintUsage",
 CellChangeTimes->{3.601554100582817*^9},
 CellTags->
  "Info3601557700-3591912",ExpressionUUID->"17fdf3e4-0c0b-4788-8f40-\
bf171806cd7b"]
}, Open  ]],

Cell[TextData[{
 "For example, the ",
 StyleBox["SunRise",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " and ",
 StyleBox["SunSet",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " commands correctly take into account atmospheric refraction. This means \
however that the true horizoncoords of the ",
 StyleBox["Sun",
  FontFamily->"Courier",
  FontWeight->"Bold"],
 " is approximately half a degree below the horizon at sunrise or sunset. I.e"
}], "Text",
 ImageRegion->{{0, 1}, {0, 
  1}},ExpressionUUID->"c33201f5-04b2-4cdd-abc4-a98a9fccc65b"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"HorizonCoordinates", "[", 
  RowBox[{"Sun", ",", 
